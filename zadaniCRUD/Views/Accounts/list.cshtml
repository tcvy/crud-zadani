@model IEnumerable<zadaniCRUD.Models.Account>

@{
    ViewBag.Title = "list";
}

<h2>Zakaznici</h2>

<p>
    @Html.ActionLink("Novy zakaznik", "Form")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ID)
        </td>
        <td>
            @Html.ActionLink("Upravit", "Form", new { id=item.ID }) 
        </td>
    </tr>
}

</table>
